{
  "name": "Amazon to Good Reads",
  "description": "Build a link from Amazon book selling page to Good Reads",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "presistent": false,
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/*"],
      "js": ["contentScript.js"]
    }
  ]
  
}